<div id="left">
	<p id="notice"><%= notice %></p>
	<h1><%= @project.name %></h1>

	<% @project.blogs.each do |blog| %>
		<h2><%= blog.name %></h2>
		<p><%= blog.message %></p>
	<% end %>
</div>
<div id="right">
	<div class="box">
		<h3>About <%= @project.name %></h3>
		<p><%= @project.description %></p>
	</div>
</div>
